{{- range $key, $value := .Site.RegularPages }}
  {{- if (len $value.Aliases) }}
  {{- printf "# %s - ./content/%s\n" $value.Title .File.Path }}
  {{- end }}
  {{- range $value.Aliases }}
     {{- $from := strings.TrimPrefix " " . }}
     {{- $from := strings.TrimPrefix "/docs" $from }}
     {{- $from := strings.TrimPrefix "/" $from }}
    {{- $to := printf "%s" (replace $value.RelPermalink "/nightly" "")}}
    {{- printf "/docs/%s %s %d\n" $from $to 301 }}
  {{- end }}
{{- end -}}


{{- with .Site.BaseURL -}}
  {{- $modifiedURL := replace . "https://tyk.io/" "" -}}
  {{- $modifiedURL = replace $modifiedURL "http://tyk.io/" "" -}}
  {{- $modifiedURL = replace $modifiedURL "//tyk.io/" "" -}}
  {{- $modifiedURL = replace $modifiedURL . "" -}}
  {{- $modifiedURL = strings.TrimSuffix "/" $modifiedURL -}}
  {{- $modifiedURL = strings.TrimSuffix "/" $modifiedURL -}}
  {{- $modifiedURL = strings.TrimPrefix "/" $modifiedURL -}}
  {{- $modifiedURL = strings.TrimPrefix "/" $modifiedURL -}}
  {{- printf "/* /%s/404.html 404\n" $modifiedURL }}
{{- end -}}