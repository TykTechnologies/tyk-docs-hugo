{{- range $key, $value := .Site.RegularPages }}
  {{- if (len $value.Aliases) }}
  {{- printf "# %s - ./content/%s\n" $value.Title .File.Path }}
  {{- end }}
  {{- range $value.Aliases }}
    {{- $from := printf "%s" . }}
    {{- $to := printf "%s" $value.RelPermalink }}
    {{- printf "/docs%s %s %d\n" $from $to 301 }}
  {{- end }}
{{- end }}
